    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-sources": "^3.2.3",
    "zip-webpack-plugin": "^4.0.1"
