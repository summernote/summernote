    "@vitest/browser": "^1.6.0",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "bootstrap": "^5.0.1",
    "chromedriver": "^126.0.4",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "cssnano": "^7.0.3",
    "eslint": "^8.13.0",
